{
  "name": "Discovery Okta Session Refresh",
  "version": "1.0",
  "description": "Discovery Okta Refresh to keep session alive",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": ["https://discovery.okta.com"],
  "manifest_version": 2,
  "icons": {
    "16": "icons/refresh-16.png",
    "32": "icons/refresh-32.png",
    "64": "icons/refresh-64.png",
    "128": "icons/refresh-128.png"
  }
}
